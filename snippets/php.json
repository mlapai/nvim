{
  "phpfile": {
    "prefix": "pfile",
    "body": [
      "<?php",
      "",
      "$0"
    ]
  },
  "phpclass": {
    "prefix": "pclass",
    "body": [
      "<?php",
      "",
      "final class ${1:className}",
      "{",
      "}$0"
    ]
  },
  "declareStrict": {
    "prefix": "dst",
    "body": [
      "declare(strict_types=1);$0"
    ]
  }
}
